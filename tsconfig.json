{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "sourceMap": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "./Sample/**/*.ts",
        "./node_modules/**/*.ts",
        "./bower_components/**/*.ts",
        "*.d.ts"
    ]
}
